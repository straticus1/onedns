{
  "version": "1.0",
  "service": "adsdnsgo",
  "api_key_id": "{{ api_key_id }}",
  "api_key": "{{ api_key }}",
  "created_at": "{{ ansible_date_time.iso8601 }}",
  "instance_id": "{{ instance_id | default('') }}",
  "provider": "oci",
  "region": "{{ oci_region | default('us-ashburn-1') }}",
  "endpoint": "http://{{ ansible_host }}:5000",
  "capabilities": [
    "dns_query",
    "dns_debug",
    "dnssec_validation",
    "spf_check",
    "dkim_check",
    "dmarc_check",
    "zone_validation"
  ]
}
